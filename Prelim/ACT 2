// Declaration
        string length;
        int cLength, yards, remainingCm, feet, cInches;

        Console.Write("Enter length (in centimeters) >>>>>>>");
        length = Console.ReadLine();
        
        cLength = Convert.ToInt32(length);
        yards = cLength / 91;
        remainingCm = cLength % 91;
        feet = remainingCm / 30;
        cInches = (int)((remainingCm % 30) / 2.54);
// Output
        Console.Write(yards + " yards, " + feet + " feet (foot), " + cInches + " inches");
